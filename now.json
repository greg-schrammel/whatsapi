{
  "version": 2,
  "name": "aham",
  "alias": ["schrammel.co", "uhul"],
  "builds": [{ "src": "web/package.json", "use": "@now/next" }],
  "routes": [
    { "src": "/w", "dest": "ws://ilikebread.herokuapp.com/w" },
    { "src": "(.*)", "dest": "web$1" }
  ],
  "env": {
    "GOOGLE_APPLICATION_CREDENTIALS": "@ilikebread-google-credentials"
  }
}
