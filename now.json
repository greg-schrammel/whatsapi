{
  "version": 2,
  "name": "aham",
  "alias": ["schrammel.co", "uhul"],
  "builds": [
    { "src": "web/package.json", "use": "@now/next" },
    { "src": "services/**/*.go", "use": "@now/go" }
  ],
  "routes": [
    {
      "src": "/whatsapp/send",
      "dest": "services/handlers/sendMessage.go"
    },
    { "src": "(.*)", "dest": "web$1" }
  ],
  "env": {
    "GOOGLE_APPLICATION_CREDENTIALS": "@ilikebread-google-credentials"
  }
}
